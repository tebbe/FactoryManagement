@using FactoryManagement.Helpers
@{
    FactoryManagement.Models.FactoryManagementEntities db = new FactoryManagement.Models.FactoryManagementEntities();
    var count = 0;
}
@if ((Enumerable.Count(ViewBag.list)) > 0)
{
    <table class="table table-hover table-bordered table-striped" id="tbl_All_his">
        <tbody>
            
            <tr>
                <th width="3%">#</th>
                <th width="25%">Voucher Name</th>
                <th>Transaction Name</th>
                <th>Machine</th>
                <th width="10%">Total Quantity</th>
                <th>Return Date</th>
                <th>Return By</th>

            </tr>
            @foreach (var item in ViewBag.list as IEnumerable<FactoryManagement.ModelView.Inventory.Store.View_Dispacth_Item_DetailsModelView>)
            {
                count++;
                <tr>
                    <th>@count.</th>
                    <td>
                        <span style="color:#6a8ff6">@item.VoucherName</span>
                    </td>
                    <td>@item.ReturnTransactionName</td>
                    <td>
                    <a class="machine_details" data-machineid="@item.MachineId">
                        @item.ReturnFrom
                        </a></td>
                    <td>
                        <span class="itemQuan" style="color:#428bca;cursor:pointer;" data-dispatchedid="@item.Id">@item.Total_Quantity @item.UnitName</span>
                    </td>
                    <td>@item.ReturnDateName</td>
                    <td><img /> @item.ReturnUserName</td>
                    @*<td>
                        @if (item.Total_Return > 0)
                        {
                            <span class="itemReQuan" style="color:#428bca;cursor:pointer;" data-invenid="@item.InventoryId" data-proname="@item.ProductName">
                                @item.Total_Return @item.UnitName
                            </span>
                        }
                        else
                        {
                            <span>@item.Total_Return @item.UnitName</span>
                        }
                    </td>*@
                   @* <td><span class="btn btn-success" data-invenid="@item.InventoryId" data-id="@item.Id"><i class="fa fa-check"></i> Accept</span></td>*@
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h4>There is no record to display</h4>
}

<script>
    totalCount = '@(ViewBag.TotalUser)';
    showCount = '@count';
</script>

