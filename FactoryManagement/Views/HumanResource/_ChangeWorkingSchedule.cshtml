
<div class="editor-label"><label>Working Schedule</label></div>
<div class="editor-field">
    @(Html.Kendo().DropDownList().Name("Working_ScheduleId")
                     .OptionLabel("--Select One Package--")
                     .BindTo(ViewBag.WorkingSchedules as SelectList)
                     .Events(e => e.Change("workingScheduleChange"))
                     .HtmlAttributes(new { style = "width:100%" })
    )
    <span class="field-validation-valid text-danger" data-valmsg-for="Working_ScheduleId" data-valmsg-replace="true"></span>
</div>
<div id="div_selected_work_scdl"></div>
<script>
    $(document).ready(function () {
        $("#div_selected_work_scdl").load('@Url.Action("SelectedSchedule","HumanResource")?id=' + '@(ViewBag.id)' + '&view=win');
    });
</script>
