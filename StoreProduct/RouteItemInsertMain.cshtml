<div class="k-edit-form-container col-lg-11 col-md-10 col-sm-11 col-xs-10" style="margin-bottom:4%;margin-top:2%;" id="div_Item_edit">
    <div id="div_RouteItem_Insert_partial"><div class="win_loading_half"></div></div>
    <div class="k-edit-buttons k-state-default col-xs-11">
        <button type="button" class="k-button k-button-icontext k-primary k-grid-update" id="btn_InsertQuan_Save"><span class="k-icon k-update"></span>OK</button>
        <button type="reset" class="k-button k-button-icontext k-grid-cancel" id="btn_InsertQuan_Cancle"><span class="k-icon k-cancel"></span>Cancel</button>
    </div>
</div>
<script>
    var id = '@(ViewBag.id)';
    $("#div_RouteItem_Insert_partial").load('@Url.Action("RouteItemInsert", "StoreProduct")?id=' + id);
    $("#btn_InsertQuan_Cancle").click(function () {
        $(this).closest(".k-window-content").data("kendoWindow").close();
    });
</script>